{
  "mcpServers": {
    "influxdb": {
      "command": "node",
      "args": ["/path/to/influx-mcp-standalone/build/index.js"],
      "env": {
        "INFLUX_DB_INSTANCE_URL": "http://localhost:8086/",
        "INFLUX_DB_TOKEN": "<YOUR_INFLUXDB_OPERATOR_TOKEN>",
        "INFLUX_DB_PRODUCT_TYPE": "core"
      }
    }
  }
}
